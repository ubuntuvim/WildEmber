{{! app/templates/pagination.hbs }}
todos count:{{model.count}}<br><br>
todos len :{{model.todos.length}}<br><br>

<table class="table">
  <thead>
    <tr>
      <th>#</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Username</th>
    </tr>
  </thead>
  <tbody>
      {{#each model.todos as |item index|}}
      {{!debugger}}
    <tr>
      <td>{{index}}</td>
      <td>{{item.id}}</td>
      <td>{{item.timestamp}}</td>
      <td>{{item.title}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>

<br>
<br>
<br>

{{!pagination-test store=store model=model}}
